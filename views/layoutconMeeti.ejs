  </head>
  <body>
    <header class="site-header contenedor">
      <a href="/">
        <img src="/img/logo.png" alt="Logo MeetUp" />
      </a>
      <nav class="nav">
        <a href="#">Crear Grupo</a>
        <a href="/iniciar-sesion">Iniciar Sesión</a>
        <a href="/crear-cuenta">Registrarse</a>
      </nav>
    </header>

    <% if (locals.mensajes) { %> <% const categoria =
    Object.keys(locals.mensajes) %> <% if(categoria.length) { %> <%
    locals.mensajes[categoria].forEach(alerta => { %>
    <div class="alertas">
      <p class="alerta <%= categoria %>"><%= alerta %></p>
    </div>
    <% }) %> <% } %> <% } %> <%- body %>

    <footer class="site-footer contenedor">
      <nav class="nav">
        <a href="/crear-grupo">Crear Grupo</a>
        <a href="/iniciar-sesion">Iniciar Sesión</a>
        <a href="/crear-cuenta">Registrarse</a>
      </nav>
      <p class="copyright">
        Todos los derechos reservados MeetUp <%- year %> &copy;
      </p>
      <!-- <p class="copyright">
        Todos los derechos reservados MeetUp <span id="year"></span> &copy;
      </p> -->
    </footer>
    <script
      type="text/javascript"
      src="https://unpkg.com/trix@2.0.0/dist/trix.umd.min.js"
    ></script>
     <script src="https://unpkg.com/leaflet@1.4.0/dist/leaflet.js"></script>
